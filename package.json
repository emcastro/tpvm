{
  "name": "tpvm",
  "version": "0.0.0",
  "description": "A Virtual Machine for TP",
  "respository": {
    "type": "git",
    "url": "https://emmanuel_castro@bitbucket.org/emmanuel_castro/tpvm.git"
  },
  "license": "MIT",
  "devDependencies": {
    "ansi-colorizer": "^1.0.1",
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-minify-constant-folding": "^0.1.1",
    "babel-plugin-minify-dead-code-elimination": "^0.1.4",
    "babel-plugin-transform-do-expressions": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-inline-consecutive-adds": "^0.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "benchmark": "^2.1.4",
    "chai": "^3.5.0",
    "child-process-data": "^0.5.1",
    "eslint": "^3.19.0",
    "eslint-plugin-flowtype": "^2.32.1",
    "event-stream": "^3.3.4",
    "flat-map": "^0.4.0",
    "flow-bin": "^0.46.0",
    "gulp": "^3.9.1",
    "gulp-antlr4": "^0.3.2",
    "gulp-clean": "^0.3.2",
    "gulp-download": "^0.0.1",
    "gulp-file": "^0.3.0",
    "gulp-sequence": "^0.4.6",
    "jest": "^20.0.3",
    "standard": "^10.0.2"
  },
  "dependencies": {
    "antlr4": "^4.7.0",
    "fs-es6-promise": "^0.0.3",
    "lodash": "^4.17.4",
    "mz": "^2.6.0"
  },
  "scripts": {
    "start": "node build/index.js",
    "build": "gulp default && babel src -d build --source-maps --copy-files",
    "pretest": "npm run build",
    "test": "standard && jest src",
    "autobuild": "babel src -d build --source-maps --copy-files --watch",
    "autocodegen": "gulp autocodegen",
    "autotest": "jest src --coverage --watchAll",
    "cleaner": "rm -rf coverage"
  },
  "standard": {
    "plugins": [
      "flowtype"
    ],
    "parser": "babel-eslint",
    "globals": [
      "test",
      "describe",
      "it",
      "expect",
      "Iterator",
      "IteratorResult",
      "$ReadOnlyArray"
    ],
    "ignore": [
      "flow-typed",
      "src/generated"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
